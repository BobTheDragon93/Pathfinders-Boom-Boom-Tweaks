				<Component Subtype="SteelPlate" Count="250" />
				 <Component Subtype="SuperComputer" Count="500" />
				<Component Subtype="Construction" Count="500" />
				<Component Subtype="MetalGrid" Count="30" />
				<Component Subtype="LargeTube" Count="150" />
				<Component Subtype="Motor" Count="150" />
				<Component Subtype="Computer" Count="150" />
				<Component Subtype="Superconductor" Count="60" />
